{
  "name": "react-chat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-socket": "^1.0.0-beta.0",
    "socket.io": "^2.0.4",
